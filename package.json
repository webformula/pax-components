{
  "name": "@webformula/pax-components",
  "version": "0.5.17-beta",
  "description": "",
  "type": "module",
  "scripts": {
    "start": "nodemon --exec \"npm run build\" & npm run serve",
    "mobile": "nodemon --exec \"npm run build:mobile\" & npm run serve",
    "kill8080": "lsof -ti:8080 | xargs kill",
    "serve": "http-server dist-docs/ -p 8081",
    "build": "npm run build:components && npm run build:css && npm run build:package && npm run build:docs",
    "build:remove-old": "rm -rf dist-docs && rm -rf dist-files ",
    "build:mobile": "rm -rf dist-docs && rm -rf dist-files && npm run build:components && npm run build:css && npm run build:package:mobile && npm run build:docs",
    "build:components": "node --experimental-modules build.js",
    "build:css": "cp dist-files/app.css dist/pax-components.css",
    "build:package": "webpack --config webpack.config.js",
    "build:package:mobile": "webpack --config webpack.config.js --mobile",
    "build:docs": "node --experimental-modules src-docs/index.js && cp -r src-docs/public/* dist-docs/ && cp dist/* dist-docs/",
    "cypress:open": "cypress open",
    "cypress:open-and-serve": "npm start & cypress open"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/webformula/pax-components.git"
  },
  "author": "Ben Rubin (B-3PO)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/webformula/pax-components/issues"
  },
  "homepage": "https://github.com/webformula/pax-components#readme",
  "devDependencies": {
    "css-loader": "^2.1.0",
    "cypress": "^3.3.0",
    "glob": "^7.1.3",
    "http-server": "^0.11.1",
    "mini-css-extract-plugin": "^0.5.0",
    "nodemon": "^1.19.1",
    "webpack": "^4.29.0",
    "webpack-cli": "^3.2.1"
  },
  "dependencies": {
    "@webformula/pax-core": "github:webformula/pax-core#v0.4.11-beta",
    "monaco-editor": "^0.18.1",
    "monaco-editor-webpack-plugin": "^1.7.0"
  }
}
