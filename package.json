{
  "name": "@webformula/pax-components",
  "version": "0.14.2-beta",
  "description": "Web components using material design language",
  "type": "module",
  "scripts": {
    "start": "node-sass src/entry.scss local/entry.css && snowpack dev --config snowpack.config.json",
    "release": "node-sass src/entry.scss local/entry.css && snowpack build --config snowpack.release.config.json && node release.js",
    "build:editor": "webpack --progress --config webpack.monaco.editor.cjs",
    "build:release": "webpack --config webpack.prod.config.cjs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/webformula/pax-components.git"
  },
  "author": "Ben Rubin (B-3PO)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/webformula/pax-components/issues"
  },
  "homepage": "https://github.com/webformula/pax-components#readme",
  "devDependencies": {
    "@snowpack/plugin-optimize": "^0.2.1",
    "@snowpack/plugin-webpack": "^1.5.0",
    "css-loader": "^4.3.0",
    "file-loader": "^6.1.0",
    "glob": "^7.1.6",
    "monaco-editor": "^0.20.0",
    "monaco-editor-webpack-plugin": "^1.9.1",
    "node-sass": "^4.14.1",
    "nodemon": "^2.0.4",
    "replace": "^1.2.0",
    "snowpack": "^2.11.1",
    "style-loader": "^1.2.1",
    "webpack": "^4.44.2",
    "webpack-cli": "^3.3.12"
  },
  "dependencies": {
    "@webformula/pax-core": "^0.12.0"
  }
}
